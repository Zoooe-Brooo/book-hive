{{#each users as |user|}}
<div class="row align-center mb-5">
  <div class="col-md-6">
    <h2>{{{get_emoji}}} {{user.name}}</h2>
  </div>
<div class="col-md-6">
    <h3><a href="{{user.email}}">{{user.email}}</a></h3>
  </div>
</div>
{{/each}}

<link rel="stylesheet" href="/css/jass.css">
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css">

    <!-- Navbar brand/title -->
    <a class="navbar-brand" href="#">{{title}}</a>


<section class="search-bar my-4"></section>
  <form class="d-flex justify-content-center" action="/search" method="GET">
    <input class="form-control w-50 me-2" type="search" placeholder="Search for books..." aria-label="Search" name="query">
    <button class="btn-search" type="submit">Search</button>
  </form>
</section>

<script src="/js/homepage.js"></script>